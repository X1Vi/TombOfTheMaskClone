; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Survivator"
run/main_scene="res://Levels/Area1/Level1.tscn"
config/icon="res://icon.png"

[autoload]

GlobalScene="*res://Scenes/GlobalScene.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"

[global]

pointing=false
import=false
etc=true

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Environement"
2d_physics/layer_2="Player"
2d_physics/layer_3="Enemy"
2d_physics/layer_4="Spikes"
2d_physics/layer_5="Spikes Area 2d"
2d_physics/layer_6="Bats"
2d_physics/layer_7="Portals"
2d_physics/layer_8="Guns"
2d_physics/layer_9="Bullet"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
